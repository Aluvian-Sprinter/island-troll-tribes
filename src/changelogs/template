//! zinc

//--ex: v288a for v. 2.88a, == {major:2, minor:88, rev:a}
library v(major)(minor)(rev) requires ChangeLog {
  function onInit() {
    ChangeLog v(major)(minor)(rev) = ChangeLog.create((major).(minor), '(rev)');
    v(major)(minor)(rev).add("1st change goes here...");
    v(major)(minor)(rev).add("2nd change goes here...");
    //...
  }
}

//! endzinc