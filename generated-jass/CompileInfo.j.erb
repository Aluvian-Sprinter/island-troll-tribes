//! zinc

library CompileInfo {

  public string CompiledBy = "jesse-win7-dt/jesse";

  public struct DateCompiled {

    static constant integer year      = 2013;
    static constant integer dayOfYear = 61;
    static constant integer month     = 3;
    static constant integer day       = 2;
    static constant integer hours     = 0;
    static constant integer minutes   = 7;
    static constant integer seconds   = 29;

    static method operator to_s() -> string {
      return I2S(year) + "-" + I2S(month) + "-" + I2S(day) + " " +
             I2S(hours) + ":" + I2S(minutes) + ":" + I2S(seconds);
    }

    static method onInit() {
      debug {
        BJDebugMsg(DateCompiled.to_s);
      }
    }
  }
}

//! endzinc