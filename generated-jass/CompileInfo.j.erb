//! zinc

library CompileInfo {

  public string CompiledBy = "quazz";

  public struct DateCompiled {

    static constant integer year      = 2013;
    static constant integer dayOfYear = 67;
    static constant integer month     = 3;
    static constant integer day       = 8;
    static constant integer hours     = 14;
    static constant integer minutes   = 26;
    static constant integer seconds   = 19;

    static method operator to_s() -> string {
      return I2S(year) + "-" + I2S(month) + "-" + I2S(day) + " " +
             I2S(hours) + ":" + I2S(minutes) + ":" + I2S(seconds);
    }

    static method onInit() {
      debug {
        BJDebugMsg("Compiled on: " + DateCompiled.to_s);
        BJDebugMsg("Compiled by: " + CompiledBy);
      }
    }
  }
}

//! endzinc