//! zinc

library CompileInfo {

  public string CompiledBy = "quazz";

  public struct DateCompiled {

    static constant integer year      = 2013;
    static constant integer dayOfYear = 35;
    static constant integer month     = 2;
    static constant integer day       = 4;
    static constant integer hours     = 21;
    static constant integer minutes   = 39;
    static constant integer seconds   = 34;

    static method operator to_s() -> string {
      return I2S(year) + "-" + I2S(month) + "-" + I2S(day) + " " +
             I2S(hours) + ":" + I2S(minutes) + ":" + I2S(seconds);
    }

    static method onInit() {
      debug {
        BJDebugMsg(DateCompiled.to_s);
      }
    }
  }

}

//! endzinc